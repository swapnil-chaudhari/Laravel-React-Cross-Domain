{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "Boilerplate for react using webpack",
  "scripts": {
    "bundle": "env NODE_ENV=production NODE_PATH=. i18n-util run webpack",
    "i18n": "npm run build && npm run i18n:export && npm run i18n:import",
    "i18n:export": "i18n-util export --project $POEDITOR_PROJECT_ID",
    "i18n:import": "i18n-util import --project $POEDITOR_PROJECT_ID",
    "lint": "npm-run-parallel lint:css lint:js",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint:js": "eslint $npm_package_config_jslintables",
    "lint:fix": "npm run lint:js:fix",
    "lint:js:fix": "eslint --fix $npm_package_config_jslintables",
    "start": "env NODE_PATH=$PWD NODE_ENV=development STANDALONE_MODE=true node lib/dev-server",
    "test": "npm-run-parallel lint test:unit",
    "test:unit": "env NODE_ENV=test NODE_PATH=. karma start karma.conf.js",
    "test:unit:watch": "npm run test:unit -- --single-run=false",
    "watch": "npm run test:unit:watch"
  },
  "config": {
    "jslintables": "src lib webpack.config.js karma.conf.js"
  },
  "author": "The Orchard",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.26.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "dirty-chai": "^1.2.2",
    "dotenv": "^2.0.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "ignore-styles": "^4.0.0",
    "import-glob-loader": "^1.1.0",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^0.13.22",
    "karma-coverage": "^1.1.1",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-notify-reporter": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "lodash.assign": "^4.2.0",
    "mocha": "^2.5.3",
    "npm-run-parallel": "^0.5.0",
    "phantomjs-prebuilt": "^2.1.16",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.6.2",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.13.2",
    "stylelint": "^6.9.0",
